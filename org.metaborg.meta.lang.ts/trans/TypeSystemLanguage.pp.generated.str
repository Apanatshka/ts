             [SOpt(HS(), "0")]
           , [a7]
           )
         ]
    where a7 := <pp-one-Z(prettyprint-Prop)> a

  prettyprint-Formula :
    Else(a, b, c) -> [ H(
                         [SOpt(HS(), "0")]
                       , [ a8
                         , S(" else error ")
                         , b2
                         , S(" on ")
                         , c0
                         ]
                       )
                     ]
    where a8 := <pp-one-Z(prettyprint-Formula)> a
    where b2 := <pp-one-Z(prettyprint-Term)> b
    where c0 := <pp-one-Z(prettyprint-Term)> c

  prettyprint-Formula :
    Or(a, b) -> [ H(
                    [SOpt(HS(), "0")]
                  , [a9]
                  )
                , H(
                    [SOpt(HS(), "0")]
                  , [S("or "), b3]
                  )
                ]
    where a9 := <pp-one-Z(prettyprint-Formula)> a
    where b3 := <pp-one-Z(prettyprint-Formula)> b

  prettyprint-Formula :
    And(a, b) -> [ H(
                     [SOpt(HS(), "0")]
                   , [a10]
                   )
                 , H(
                     [SOpt(HS(), "0")]
                   , [S("and "), b4]
                   )
                 ]
    where a10 := <pp-one-Z(prettyprint-Formula)> a
    where b4 := <pp-one-Z(prettyprint-Formula)> b

  prettyprint-Formula :
    Label(a, b) -> [ H(
                       [SOpt(HS(), "0")]
                     , [a11, S(" :: "), b5]
                     )
                   ]
    where a11 := <pp-one-Z(prettyprint-LId)> a
    where b5 := <pp-one-Z(prettyprint-Formula)> b

  prettyprint-Formula :
    Parenthetical(a) -> [ H(
                            [SOpt(HS(), "0")]
                          , [S("("), a12, S(")")]
                          )
                        ]
    where a12 := <pp-one-Z(prettyprint-Formula)> a

  prettyprint-example =
    prettyprint-Formula


