    where not(is-PPTerm)
    where t0__' := <pp-one-Z(prettyprint-StringQuotation)> t0__

  prettyprint-StringQuotedPart1 :
    StringEscape1(t0__, t1__, t2__, t3__) -> [ H(
                                                 [SOpt(HS(), "0")]
                                               , [ t0__'
                                                 , S("[")
                                                 , t1__'
                                                 , t2__'
                                                 , t3__'
                                                 , S("]")
                                                 ]
                                               )
                                             ]
    with t0__' := <pp-one-Z(prettyprint-Padding)> t0__
    with t1__' := <pp-one-Z(prettyprint-OptLAYOUT)> t1__
    with t2__' := <pp-one-Z(prettyprint-Term)> t2__
    with t3__' := <pp-one-Z(prettyprint-OptLAYOUT)> t3__

  is-StringQuotedPart1 =
