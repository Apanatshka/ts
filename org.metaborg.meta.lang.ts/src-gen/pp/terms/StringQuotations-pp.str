    with t1__' := <pp-one-Z(prettyprint-OptLAYOUT)> t1__
    with t2__' := <pp-one-Z(prettyprint-Term)> t2__
    with t3__' := <pp-one-Z(prettyprint-OptLAYOUT)> t3__

  is-StringQuotedPart4 =
