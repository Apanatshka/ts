  prettyprint-Padding :
    t0__ -> [ H(
                [SOpt(HS(), "0")]
              , [t0__']
              )
            ]
    where not(is-Padding)
    where t0__' := <pp-one-Z(prettyprint-Padding)> t0__

  prettyprint-OptLAYOUT :
    None() -> [ H(
                  []
                , [S("")]
                )
              ]

  is-OptLAYOUT =
